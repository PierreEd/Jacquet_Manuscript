@article{Kokubun1987,
   abstract = {The impedance dependence of a tuning-fork-shaped quartz oscillator on gas pressure is theoretically analy7..ed on the basis of a string-of-beads model of the quartz oscillator. The theoretical results are numerically compared with experimental data. The analysis has been already carried out in molecular flow and viscous flow region. In this paper, the impedance behavior in the intermediate flow region is formulated by making usc of the theory of slip effect. Furthermore, by the formal extension of this formula to the molecular flow region, a single formula describing the impedance dependence on gas pressure over the whole pressure region is obtained. This formula is found to be in good agreement with experimental data. However, there is still a slight discrepancy ( < 20%) between the two in the intermediate flow region. This shows the limit of. the slip theory. For this reason, the Millikan's empirical fonnula is adopted in the intermediate flow region.· The unified formula obtained thusly is in numerical agreement with experimenta! data for various kinds of gases.},
   author = {K. Kokubun},
   doi = {10.1116/1.574869},
   issn = {07342101},
   issue = {4},
   journal = {Journal of Vacuum Science & Technology A: Vacuum, Surfaces, and Films},
   pages = {2450},
   title = {Unified formula describing the impedance dependence of a quartz oscillator on gas pressure},
   volume = {5},
   year = {1987},
}
