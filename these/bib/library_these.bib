@article{Buters2016,
   abstract = {Experiments in the field of optomechanics do not yet fully exploit the photon polarization degree of freedom. Here experimental results for an optomechanical interaction in a polarization nondegenerate system are presented and schemes are proposed for how to use this interaction to perform accurate side-band thermometry and to create novel forms of photon-phonon entanglement. The experimental system utilizes the compressive force in the mirror attached to a mechanical resonator to create a micro-mirror with two radii of curvature which leads, when combined with a second mirror, to a significant polarization splitting of the cavity modes.},
