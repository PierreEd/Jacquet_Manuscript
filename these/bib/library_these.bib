   keywords = {Electron Spin Resonance Spectroscopy,Electron Spin Resonance Spectroscopy: instrumentat,Electron Spin Resonance Spectroscopy: methods,Equipment Design,Equipment Design: methods,Equipment Failure Analysis,Mathematical Computing,Mechanics,Nanotechnology,Nanotechnology: instrumentation,Nanotechnology: methods,Transducers},
   month = {1},
   pages = {231},
   pmid = {21407197},
   publisher = {Nature Publishing Group},
   title = {Phonon-tunnelling dissipation in mechanical resonators.},
   volume = {2},
   url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3072094&tool=pmcentrez&rendertype=abstract},
   year = {2011},
}
