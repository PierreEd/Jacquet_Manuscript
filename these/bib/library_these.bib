   keywords = {Fluctuations,Optical microelectromechanical devices,Phase measurement,and noise,relaxations},
   month = {10},
   pages = {23236},
   publisher = {OSA},
   title = {Determination of the vacuum optomechanical coupling rate using frequency noise calibration},
   volume = {18},
