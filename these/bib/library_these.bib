   journal = {Opt Express},
   pages = {5658-5669},
   pmid = {21445206},
   title = {Quasi-two-dimensional optomechanical crystals with a complete phononic bandgap},
   volume = {19},
   url = {<Go to ISI>://WOS:000288871300106},
   year = {2011},
}
@article{Stomeo2010,
   abstract = {We demonstrate an ultracompact optical filter based on two coupled\nhigh-index contrast GaAs photonic crystal (PhC) membranes. The PhC\nmembranes consist of a square lattice of air holes and behave as\na Fabry-Perot cavity whose reflectivity and transmissivity depend\non the air gap between the two membranes. The normal-incidence reflectance\nmeasurements and the numerical simulation of reflection spectra show\na high sensitivity to the geometrical parameters, such as the distance\nbetween the slabs, whose control would make the device suitable for\na new class of tunable optical filters. (C) 2010 Optical Society\nof America},
   author = {T Stomeo and M Grande and G Raino and A Passaseo and A D'Orazio and R Cingolani and A Locatelli and D Modotto and C De Angelis and M De Vittorio},
