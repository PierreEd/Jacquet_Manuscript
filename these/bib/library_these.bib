   issn = {1570-0755},
   issue = {2-3},
   journal = {Quantum Information Processing},
   keywords = {Adenosine Diphosphate,Adenosine Diphosphate: pharmacology,Animals,Binding Sites,Cattle,Glutamate Dehydrogenase,Glutamate Dehydrogenase: metabolism,Glutamates,Guanosine Triphosphate,Guanosine Triphosphate: pharmacology,Kinetics,Liver,Liver: enzymology,NAD,NADP,Protein Binding,Spectrophotometry, Ultraviolet,Time Factors},
   month = {2},
   pages = {81-103},
   pmid = {39},
   title = {Superconducting phase qubits},
   volume = {8},
   url = {http://pubget.com/paper/pgtmp_ceb39e5e871858a806bbcc0092517075/Superconducting_phase_qubits?from=pgtmp_08094685&q=latest:Physics+Reports},
