   month = {9},
   pages = {033814},
   publisher = {American Physical Society},
   title = {Quantum dynamics of an optical cavity coupled to a thin semitransparent membrane: Effect of membrane absorption},
   volume = {84},
   url = {https://journals.aps.org/pra/abstract/10.1103/PhysRevA.84.033814},
   year = {2011},
}
@article{Unterreithmeier2010,
   abstract = {We study the transverse oscillatory modes of nanomechanical silicon nitride strings under high tensile stress as a function of geometry and mode index m≤9. Reproducing all observed resonance frequencies with classical elastic theory we extract the relevant elastic constants. Based on the oscillatory local strain we successfully predict the observed mode-dependent damping with a single frequency-independent fit parameter. Our model clarifies the role of tensile stress on damping and hints at the underlying microscopic mechanisms. © 2010 The American Physical Society.},
   author = {Quirin P. Unterreithmeier and Thomas Faust and Jörg P. Kotthaus},
   doi = {10.1103/PHYSREVLETT.105.027205/FIGURES/3/MEDIUM},
