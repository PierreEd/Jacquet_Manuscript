}
@article{Bahrami2014,
   abstract = {The test of modifications to quantum mechanics aimed at identifying the fundamental reasons behind the unobservability of quantum mechanical superpositions at the macroscale is a crucial goal of modern quantum mechanics. Within the context of collapse models, current proposals based on interferometric techniques for their falsification are far from the experimental state of the art. Here we discuss an alternative approach to the testing of quantum collapse models that, by bypassing the need for the preparation of quantum superposition states might help us addressing nonlinear stochastic mechanisms such as the one at the basis of the continuous spontaneous localization model.},
