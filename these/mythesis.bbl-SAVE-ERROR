      \endverb
      \verb{url}
      \verb https://opg.optica.org/optica/abstract.cfm?URI=optica-10-3-364
      \endverb
      \keyw{Laser ablation; Laser cooling; Metal nanoparticles; Optical trapping; Phase space analysis methods; Quantum technology}
