      \strng{namehash}{94c75496a854a6b59cec4c9430113db9}
      \strng{fullhash}{6ffb59a358fb052f5aa81c306faea3c4}
      \strng{fullhashraw}{6ffb59a358fb052f5aa81c306faea3c4}
      \strng{bibnamehash}{94c75496a854a6b59cec4c9430113db9}
      \strng{authorbibnamehash}{94c75496a854a6b59cec4c9430113db9}
